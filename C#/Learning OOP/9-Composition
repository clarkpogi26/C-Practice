using System;
using System.Runtime.CompilerServices;
using System.Xml.Linq;
using C__starter;

namespace C__starter
{
    class Program
    {
        static void Main(string[] args)
        {
            Car car = new Car();
            car.start();
        }
    }
    class Engine
    {
        public void start()
        {
            Console.WriteLine("Engine Started");
        }
    }
    class Car
    {
        Engine engine = new Engine();
        public void start()
        {
            engine.start();
        }
    }
}
